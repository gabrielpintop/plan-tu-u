{
  "name": "plan-tu-u",
  "author": "Gabriel Pinto & Vivian Gómez",
  "license": "MIT",
  "description": "Esta aplicación busca fidelizar a los egresados uniandinos mediante un sistema de puntos a partir de los cuales ellos obtienen beneficios. La aplicación permite que puedan ver los beneficios que pueden obtener y que administren sus puntos",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/glpinto10/plan-tu-u.git"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "cryptr": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "meteor-node-stubs": "^0.4.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-loading-screen": "0.0.17",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.0"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    },
    "testModule": "tests/main.js"
  }
}
